public class ColumnsException extends Exception {
	
	private static final long serialVersionUID = -7571828329590359021L;

	public ColumnsException(String message) {
		super(message);
	}

	public ColumnsException(String message, Throwable throwable) {
		super(message, throwable);
	}
}
